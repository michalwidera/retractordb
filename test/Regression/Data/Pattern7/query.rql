# Check if fields are identified correctly - Covering #issue_17

DECLARE a BYTE, b BYTE STREAM coreAB, 1 FILE '/dev/urandom'
DECLARE c BYTE, d BYTE STREAM coreCD, 0.5 FILE '/dev/urandom'

SELECT coreAB[0],coreCD[0] STREAM out1 FROM coreAB+coreCD
SELECT * STREAM out2 FROM coreAB+coreCD
SELECT out1[0],out1[1] STREAM out3 FROM out1